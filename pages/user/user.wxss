/**index.wxss**/

@import "../../wxParse/wxParse.wxss"; 
.banner{
	height:430rpx;
	width:100%;
	overflow:hiddeen;
	position:relative;
}
.banner image{
	width:100%;
	height:430rpx;
}
.user_top{
	width:100%;
	height:100%;
	position:absolute;
	top:60rpx;
}
.user_img{
	width:132rpx;
	height:132rpx;
	border-radius:50%;
	overflow:hidden;
	margin:0px auto;
	border:solid 5rpx #01bdea;
}
.member_name{
	font-szie:36rpx;
	color:#434343;
	text-align:center;
	height:90rpx;
	line-height:90rpx;
}
.member_phone{
	font-size:26rpx;
	color:#999;
	text-align:center;
}
#user{
	padding:0 6.6%;
	width:86.8%;
	flex-wrap:wrap;
	margin-bottom:40rpx;
}
.user_item{
	height:200rpx;
	padding:50rpx 0;
	width:46%;
	border-radius:15rpx;
	text-align:center;
	margin-top:40rpx;
}
.user_item image{
	width:90rpx;
	height:90rpx;
}
.user_txt{
	font-size:26rpx;
	color:#666;
	height:60rpx;
}
.agent_cont{
	width:80%;
	margin:0 auto;
}